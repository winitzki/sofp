\begin_layout Plain Layout

final case class Matrix[N, A](byIndex: ((N, N)) => A) extends SqSize[N,
 A]
\end_layout

\begin_layout Plain Layout

final case class Next[N, A](next: SqSize[Option[N], A])  extends SqSize[N,
 A]
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

type Sq[A] = SqSize[Unit, A]
\end_layout

\end_inset


\end_layout

