\[
\text{seq}_{L}^{F,A}:L^{F^{A}}\rightarrow F^{L^{A}}\cong Z\rightarrow F^{Z}\quad,\quad\quad\text{seq}_{L}^{F,A}\triangleq\text{pu}_{F}^{:Z\rightarrow F^{Z}}\quad.
\]

