\begin_inset Formula 
\begin{align*}
 & \text{pu}_{\text{List}}\bef\text{reduceList}=(m^{:M}\rightarrow\bbnum 0+m\times\left[\right])\bef\text{reduceList}\\
 & =\,\begin{array}{|c||cc|}
