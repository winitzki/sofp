\begin_layout Plain Layout

implicit class TraversableScanOps[L[_] : Traversable, A](la: L[A]) {
\end_layout

\begin_layout Plain Layout

  def scanLeft[Z](init: Z)(f: (A, Z) => Z): L[Z] = la.traverse(a => accum(a,
 f)).run(init)._1
\end_layout

\begin_layout Plain Layout

}
\end_layout

